{
  "name": "integration-test",
  "version": "1.0.0",
  "description": "App for integration tests",
  "scripts": {
    "clean": "rimraf aot/ dist/ node_modules/ng-app-state/",
    "build": "tsc -p src/",
    "build:watch": "tsc -p src/ -w",
    "build:e2e": "tsc -p e2e/",
    "build:aot": "node build.js",
    "serve": "lite-server -c=bs-config.json",
    "serve:aot": "lite-server -c bs-config.aot.json",
    "serve:e2e": "lite-server -c=bs-config.e2e.json",
    "serve:e2e-aot": "lite-server -c bs-config.e2e-aot.json",
    "prestart": "yarn build",
    "start": "concurrently \"yarn build:watch\" \"yarn serve\"",
    "pree2e": "yarn build:e2e && yarn build",
    "e2e": "concurrently \"yarn serve:e2e\" \"yarn protractor\" --kill-others --success first",
    "pree2e:aot": "yarn build:e2e && yarn build:aot",
    "e2e:aot": "concurrently \"yarn serve:e2e-aot\" \"yarn protractor\" --kill-others --success first",
    "preprotractor": "webdriver-manager update",
    "protractor": "protractor protractor.config.js"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@angular/common": "^5.2.9",
    "@angular/compiler": "^5.2.9",
    "@angular/compiler-cli": "^5.2.9",
    "@angular/core": "^5.2.9",
    "@angular/forms": "^5.2.9",
    "@angular/platform-browser": "^5.2.9",
    "@angular/platform-browser-dynamic": "^5.2.9",
    "@ngrx/store": "^5.2.0",
    "core-js": "^2.4.1",
    "micro-dash": "^3.3.0",
    "ng-app-state": "../dist/",
    "rxjs": "^5.5.8",
    "systemjs": "0.19.40",
    "zone.js": "^0.8.4"
  },
  "devDependencies": {
    "@types/jasmine": "^2.8.6",
    "concurrently": "^3.4.0",
    "glob": "^7.1.1",
    "jasmine-core": "^3.1.0",
    "lite-server": "^2.2.2",
    "protractor": "^5.3.1",
    "rimraf": "^2.5.4",
    "rollup": "^0.42.0",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-node-resolve": "3.0.0",
    "rollup-plugin-uglify": "^2.0.1",
    "typescript": "~2.5.3"
  },
  "repository": {}
}
