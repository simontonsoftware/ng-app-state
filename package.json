{
  "name": "ng-app-state",
  "version": "4.1.0",
  "description": "",
  "main": "./bundles/ng-app-state.umd.js",
  "module": "./ng-app-state.es5.js",
  "es2015": "./ng-app-state.js",
  "typings": "./ng-app-state.d.ts",
  "author": "Simonton Software",
  "license": "MIT",
  "repository": "simontonsoftware/ng-app-state",
  "engines": {
    "node": ">= 6.9.0",
    "npm": ">= 3.0.0"
  },
  "scripts": {
    "clean": "rimraf out-tsc dist/*",
    "prebuild": "yarn run clean",
    "build": "node build.js",
    "serve": "lite-server -c=bs-config.json",
    "build-test": "tsc -p lib/tsconfig.spec.json",
    "build-test:watch": "tsc -p lib/tsconfig.spec.json -w",
    "pretest": "yarn build-test",
    "test": "concurrently \"yarn build-test:watch\" \"karma start karma.conf.js\"",
    "precoverage": "yarn build-test",
    "coverage": "karma start karma-coverage.conf.js --single-run --browsers Chrome",
    "pretest:once": "yarn build-test",
    "test:once": "karma start karma.conf.js --single-run --browsers Chrome",
    "preintegration": "yarn build && cd integration && yarn clean && npm install",
    "integration": "yarn integration:aot && yarn integration:jit",
    "integration:jit": "cd integration && yarn e2e",
    "integration:aot": "cd integration && yarn e2e:aot",
    "lint": "tslint ./lib/**/*.ts -t verbose",
    "release": "standard-version && yarn run build"
  },
  "peerDependencies": {
    "@angular/core": ">=5.0.0 <6.0.0",
    "@ngrx/store": ">=4.0.0 <5.0.0",
    "micro-dash": ">=1.0.0 <2.0.0",
    "rxjs": ">=5.5.0 <6.0.0"
  },
  "devDependencies": {
    "@angular/common": "^5.0.0",
    "@angular/compiler": "^5.0.0",
    "@angular/compiler-cli": "^5.0.0",
    "@angular/core": "^5.0.0",
    "@angular/platform-browser": "^5.0.0",
    "@angular/platform-browser-dynamic": "^5.0.0",
    "@ngrx/store": "^5.0.0",
    "@types/jasmine": "~2.8.3",
    "@types/node": "~6.0.60",
    "camelcase": "^4.0.0",
    "concurrently": "^3.5.0",
    "core-js": "^2.4.1",
    "coveralls": "^3.0.0",
    "glob": "^7.1.1",
    "jasmine-core": "~2.8.0",
    "karma": "~2.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "^1.0.1",
    "karma-coverage": "^1.1.1",
    "karma-html-reporter": "^0.2.7",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-remap-coverage": "^0.1.4",
    "lite-server": "^2.2.2",
    "micro-dash": "^3.2.0",
    "rimraf": "^2.6.1",
    "rollup": "~0.45.2",
    "rollup-plugin-commonjs": "~8.2.6",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-sourcemaps": "^0.4.1",
    "rollup-plugin-uglify": "^2.0.1",
    "rxjs": "^5.5.2",
    "standard-version": "^4.0.0",
    "systemjs": "^0.20.17",
    "tslib": "^1.8.0",
    "tslint": "~5.9.1",
    "typescript": "~2.5.3",
    "zone.js": "^0.8.4"
  }
}
